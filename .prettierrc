{
  "semi": true,
  "singleQuote": false,
  "trailingComma": "es5",
  "printWidth": 80,
  "tabWidth": 4,
  "arrowParens": "avoid",
  "endOfLine": "lf",
  "useTabs": true,
  "overrides": [
    {
      "files": ["*.json", ".prettierrc"],
      "options": {
        "useTabs": false,
        "tabWidth": 2
      }
    }
  ]
}
